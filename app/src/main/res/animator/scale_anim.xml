<?xml version="1.0" encoding="utf-8"?>


<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:ordering="sequentially">
    <objectAnimator
        android:duration="500"
        android:propertyName="scaleX"
        android:valueTo="1.5"
        android:valueType="floatType" />

    <objectAnimator
        android:duration="500"
        android:propertyName="scaleX"
        android:valueTo="1"
        android:valueType="floatType" />

    <objectAnimator
        android:duration="500"
        android:propertyName="scaleX"
        android:valueTo="1.5"
        android:valueType="floatType" />
</set>

